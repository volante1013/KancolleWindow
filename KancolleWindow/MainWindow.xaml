<Window x:Class="KancolleWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KancolleWindow"
        mc:Ignorable="d"
        Title="KancolleWindow" Height="777" Width="1216">
    <Grid>
        <DockPanel x:Name="DockPanel" LastChildFill="True">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="ミュート : OFF" Click="Mute_Click"/>
                <MenuItem Header="最前面 : OFF" Click="TopMost_Click"/>
                <MenuItem Header="スクショ" Click="ScreenShot_Click"/>
                <MenuItem Header="リロード" Click="Reload_Click"/>
                <MenuItem Header="その他">
                    <MenuItem Header="AAA"/>
                </MenuItem>
            </Menu>
        </DockPanel>
    </Grid>
</Window>
